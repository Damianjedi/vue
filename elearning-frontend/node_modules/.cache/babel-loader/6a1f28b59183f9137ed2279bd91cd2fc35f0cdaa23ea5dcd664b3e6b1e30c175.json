{\r\n      try {\r\n        const response = await axios.get('/api/elearning/posts');\r\n        this.posts = response.data;\r\n      } catch (error) {\r\n        this.error = 'Error fetching posts';\r\n      }\r\n    },\r\n    async createPost() {\r\n      try {\r\n        const response = await axios.post('/api/elearning/posts', { content: this.newPostContent });\r\n        this.posts.push(response.data);\r\n        this.newPostContent = '';\r\n      } catch (error) {\r\n        this.error = 'Error creating post';\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.post {\r\n  border: 1px solid #ccc;\r\n  padding: 1rem;\r\n  margin: 1rem 0;\r\n}\r\n\r\n.error {\r\n  color: red;\r\n}\r\n</style>\r\n"],"mappings":";AAgBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAC,UAAU;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,cAAc,EAAE,EAAE;MAClBC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,UAAU,CAAC,CAAC;EACzB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMT,KAAK,CAACU,GAAG,CAAC,sBAAsB,CAAC;QACxD,IAAI,CAACP,KAAI,GAAIM,QAAQ,CAACP,IAAI;MAC5B,EAAE,OAAOG,KAAK,EAAE;QACd,IAAI,CAACA,KAAI,GAAI,sBAAsB;MACrC;IACF,CAAC;IACD,MAAMM,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMF,QAAO,GAAI,MAAMT,KAAK,CAACY,IAAI,CAAC,sBAAsB,EAAE;UAAEC,OAAO,EAAE,IAAI,CAACT;QAAe,CAAC,CAAC;QAC3F,IAAI,CAACD,KAAK,CAACW,IAAI,CAACL,QAAQ,CAACP,IAAI,CAAC;QAC9B,IAAI,CAACE,cAAa,GAAI,EAAE;MAC1B,EAAE,OAAOC,KAAK,EAAE;QACd,IAAI,CAACA,KAAI,GAAI,qBAAqB;MACpC;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}